
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  user_id    String    @id(map: "user_id_prikey") @db.Char(10)
  username   String?   @unique(map: "username_unique") @db.Char(99)
  password   String?   @db.Char(99)
  created_at DateTime? @default(now()) @db.Timestamp(6)
  updated_at DateTime? @default(now()) @db.Timestamp(6)
}

